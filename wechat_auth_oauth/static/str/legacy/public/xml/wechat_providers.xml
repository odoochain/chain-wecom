<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">


    <t t-name="wechat_auth_oauth.QrCodeDialog">
        <div class="wechat_qr_dialog modal fade" id="wechat_qr_dialog" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static">
            <div class="modal-dialog" style="top:50px;">
                <div class="modal-content border-0" style="background-color:transparent">
                    <main class="modal-body d-flex justify-content-center" style="background-color:#FFFFFF">
                        <iframe t-att-src="url" frameborder="0" scrolling="0" loading="eager" height="400"></iframe>
                    </main>
                    <footer class="modal-footer border-0" style="justify-content:center;background-color:transparent">
                        <button type="button" class="btn btn-light btn-lg border-0" data-bs-dismiss="modal" style="background-color:transparent">
                            <i class="fa fa-times-circle-o" aria-label="Close" style="background-color:transparent;font-size: 36px;color: #ffffff;"/>
                        </button>
                    </footer>
                </div>
            </div>
        </div>
    </t>




</templates>