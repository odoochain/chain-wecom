<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="wechatmessage_templates_view_tree" model="ir.ui.view">
            <field name="name">wechat.message_templates.view.tree</field>
            <field name="model">wechat.message_templates</field>
            <field name="arch" type="xml">
                <tree string="">
                    <field name="name" />
                    <field name="model_id"/>
                    <field name="subject" />
                    <!-- <field name="msgtype" />
                    <field name="partner_to" /> -->
                </tree>
            </field>
        </record>

        <record id="wechat_message_templates_action" model="ir.actions.act_window">
            <field name="name">Message Templates</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">wechat.message_templates</field>
            <field name="view_mode">tree,form</field>
            <field name="target">current</field>
            <!-- <field name="context">{'active_test': False}</field> -->
        </record>

    </data>
</odoo>