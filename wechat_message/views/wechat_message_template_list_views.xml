<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="wechatmessage_template_list_view_tree" model="ir.ui.view">
            <field name="name">wechat.message_templates.view.tree</field>
            <field name="model">wechat.message_template_list</field>
            <field name="arch" type="xml">
                <tree create="0" edit="0" delete="0" js_class="wechat_message_template_list">
                    <field name="name" />
                    <field name="template_id"/>
                    <field name="title" />
                </tree>
            </field>
        </record>

        <record id="wechat_message_template_list_action" model="ir.actions.act_window">
            <field name="name">Wechat Message Template List</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">wechat.message_template_list</field>
            <field name="view_mode">tree,form</field>
            <field name="target">current</field>
            <!-- <field name="context">{'active_test': False}</field> -->
        </record>


    </data>
</odoo>